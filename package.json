{
  "name": "repository-concierge",
  "version": "0.1.0",
  "description": "Your repository concierge. Able to help you manage numerous projects, stay up to date with security, and make you laugh. Service with a smile.",
  "main": "index.js",
  "scripts": {
    "build": "npm run lint && npm run build-server && npm run build-client && ls -l",
    "build-client": "webpack && copyfiles -f dist/**/* build/dist && rimraf dist",
    "build-server": "tsc && copyfiles -f src/server/assets/* build/assets --error --verbose",
    "develop": "npm run build-server && env-cmd node build/index.js",
    "format": "prettier --write .",
    "lint": "eslint ./src",
    "start": "node build/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexlee-dev/repository-concierge.git"
  },
  "keywords": [
    "repository"
  ],
  "author": "Alex Lee (www.alexlee.dev)",
  "license": "GPL-3.0+",
  "bugs": {
    "url": "https://github.com/alexlee-dev/repository-concierge/issues"
  },
  "homepage": "https://github.com/alexlee-dev/repository-concierge#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-rate-limit": "^5.1.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/mongodb": "^3.6.8",
    "@types/mongoose": "^5.10.3",
    "@types/morgan": "^1.9.2",
    "@types/node-fetch": "^2.5.8",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack-dev-middleware": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "clean-webpack-plugin": "^3.0.0",
    "copyfiles": "^2.4.1",
    "css-loader": "^5.1.1",
    "env-cmd": "^10.1.0",
    "eslint": "^7.21.0",
    "html-webpack-plugin": "^5.2.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.17",
    "typescript": "^4.2.2",
    "webpack": "^5.24.3",
    "webpack-dev-middleware": "^4.1.0"
  },
  "dependencies": {
    "@octokit/rest": "^18.3.3",
    "antd": "^4.15.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "date-fns": "^2.19.0",
    "express": "^4.17.1",
    "express-rate-limit": "^5.2.6",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.4",
    "mongoose": "^5.11.19",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0"
  }
}
